{
  "name": "Go",
  "dockerFile": "Dockerfile",
  "runArgs": [
    "-u",
    "vscode",
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "extensions": [
    "golang.go",
    "eamodio.gitlens",
    "esbenp.prettier-vscode",
    "redhat.vscode-yaml",
    "vscode-icons-team.vscode-icons"
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
    "go.autocompleteUnimportedPackages": true
  }
}
